                                android:layout_height="wrap_content"
                                android:layout_marginTop="7dp"
                                android:layout_marginEnd="8dp"
                                android:layout_marginBottom="7dp"
                                android:background="@drawable/rectangle"
                                android:ems="5"
                                android:hint="123"
